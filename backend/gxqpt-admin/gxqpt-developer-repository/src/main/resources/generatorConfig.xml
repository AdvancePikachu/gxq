 		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://127.0.0.1:3306/gxqpt_core_zs"
                        userId="root" password="root"></jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!--
         新增7个属性，用于修改Example类生成时的存放路径 by：tangyh
         [javaModelGenerator]:
         targetExamplePackage: Example类的存放包名
         targetExampleProject: Example类的存放项目路径
         targetExampleSuperClass: Example的父类完整类名
         targetGeneratedCriteriaSuperClass: Example内部类GeneratedCriteria 的父类完整类名
         rootClass: 实体父类

         [javaClientGenerator]:
         targetIsGenMethod: 是否在dao中生成扩展的方法
         targetSuperClass:  dao父类接口
         targetSuperGeneric: dao泛型第一个参数

         [table]:
         enabledGxqptExt 是否在xml中生成扩展的方法
         -->

        <javaModelGenerator targetPackage="com.hengyunsoft.platform.developer.entity.service.po"
                            targetProject="src\main\java"
                            targetExamplePackage="com.hengyunsoft.platform.developer.repository.service.example"
                            targetExampleProject="src\main\java"
                            targetExampleSuperClass="com.hengyunsoft.db.example.BaseExample"
                            targetGeneratedCriteriaSuperClass="com.hengyunsoft.db.example.BaseGeneratedCriteria" >
            <!-- 指定service接口要继承的接口 -->
            <property name="serviceSuperInterface" value="com.hengyunsoft.base.service.normal.BaseService"/>
            <property name="serviceProject" value="src\main\java"/>
            <!-- 指定service接口所在的包 -->
            <property name="servicePackage" value="com.hengyunsoft.platform.developer.repository.service.service"/>
            <!-- 指定service实现所要继承的父类 -->
            
            <property name="serviceImplSuperClass" value="com.hengyunsoft.base.service.impl.BaseServiceImpl"/>
            <!-- 指定service实现所在的包 -->
            
            <property name="serviceImplPackage" value="com.hengyunsoft.platform.developer.repository.service.service.impl"/>         
            <property name="rootClass" value="com.hengyunsoft.base.entity.BaseEntity&lt;Long&gt;"/>
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="mapper_developer.service.base" targetProject="src\main\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.hengyunsoft.platform.developer.repository.service.dao"
                             targetProject="src\main\java"
                             targetIsGenMethod="false"
                             targetSuperClass="com.hengyunsoft.base.dao.BaseNormalDao"
                             targetSuperGeneric="Long"
        >
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="c_service_assign_module" domainObjectName="ServiceAssignModule" enabledGxqptExt="true"></table>
        <table tableName="c_service_module" domainObjectName="ServiceModule" enabledGxqptExt="true"></table>
